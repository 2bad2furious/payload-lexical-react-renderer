{
  "name": "@atelier-disko/payload-lexical-react-renderer",
  "version": "0.0.3",
  "type": "module",
  "types": "src/payloadLexicalReactRenderer.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "[ \"$NODE_ENV\" = production ] && exit 0; husky install",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "main": "dist/index.cjs",
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md",
    "src/payloadLexicalReactRenderer.d.ts"
  ],
  "exports": {
    "types": "./dist/payloadLexicalReactRenderer.d.ts",
    "import": "./dist/payload-lexical-react-renderer.js",
    "require": "./dist/payload-lexical-react-renderer.cjs"
  },
  "keywords": [
    "payload",
    "cms",
    "typscript",
    "lexical",
    "react",
    "richtext"
  ]
}
